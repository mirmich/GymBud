{
  "expo": {
    "name": "GymLog",
    "slug": "GymBud",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/logo-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mirekt.GymBud"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo-2.png",
        "backgroundColor": "#000"
      },
      "package": "com.mirekt.GymBud"
    },
    "web": {
      "favicon": "./assets/icons/logo-2.png"
    },
    "extra": {
      "eas": {
        "projectId": "642e7d35-d852-4388-8df0-545c7bac521c"
      }
    },
    "plugins": [
      ["@morrowdigital/watermelondb-expo-plugin"],
      "expo-font",
      "expo-build-properties"
    ]
  }
}
